<?xml version="1.0"?>
<cd:Record format="binary" name="GOM_EXT_2P_ADSR_residual_extinction_v1" last-modified="2010-08-26" xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07">
  <cd:Field name="dsr_time">
    <cd:Time timeformat="binary_envisat_datetime">
      <cd:Description>Start time of the measurement</cd:Description>
    </cd:Time>
  </cd:Field>
  <cd:Field name="attach_flag">
    <cd:Integer>
      <cd:Description>Attachment Flag (set to 1 if all MDSRs corresponding to this ADSR are blank, set to zero otherwise)</cd:Description>
      <cd:BitSize>8</cd:BitSize>
      <cd:NativeType>uint8</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="lat">
    <cd:Integer>
      <cd:Description>Latitude of the spacecraft value provided during the measurement, generally at half-measurement (the temporal shift is given by the time shift for the ray tracing in the SPH)</cd:Description>
      <cd:Unit>1e-6 degrees_north</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="1000000.000000">
        <cd:Unit>degrees_north</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="longit">
    <cd:Integer>
      <cd:Description>Longitude of the spacecraft value provided during the measurement, generally at half-measurement (the temporal shift is given by the time shift for the ray tracing in the SPH)</cd:Description>
      <cd:Unit>1e-6 degrees_east</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="1000000.000000">
        <cd:Unit>degrees_east</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="alt">
    <cd:Integer>
      <cd:Description>Altitude of the spacecraft value provided during the measurement, generally at half-measurement (the temporal shift is given by the time shift for the ray tracing in the SPH)</cd:Description>
      <cd:Unit>1e-2 m</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="100.000000">
        <cd:Unit>m</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>uint32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="tangent_lat">
    <cd:Integer>
      <cd:Description>Latitude of the tangent point value provided during the measurement, generally at half-measurement (the temporal shift is given by the time shift for the ray tracing in the SPH)</cd:Description>
      <cd:Unit>1e-6 degrees_north</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="1000000.000000">
        <cd:Unit>degrees_north</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="tangent_long">
    <cd:Integer>
      <cd:Description>Longitude of the tangent point value provided during the measurement, generally at half-measurement (the temporal shift is given by the time shift for the ray tracing in the SPH)</cd:Description>
      <cd:Unit>1e-6 degrees_east</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="1000000.000000">
        <cd:Unit>degrees_east</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="tangent_alt">
    <cd:Integer>
      <cd:Description>Altitude of the tangent point value provided during the measurement, generally at half-measurement (the temporal shift is given by the time shift for the ray tracing in the SPH)</cd:Description>
      <cd:Unit>1e-2 m</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="100.000000">
        <cd:Unit>m</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>uint32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="err_tangent_lat">
    <cd:Integer>
      <cd:Description>Error on the latitude of the tangent point value provided during the measurement, generally at half-measurement (the temporal shift is given by the time shift for the ray tracing in the SPH)</cd:Description>
      <cd:Unit>1e-7 degrees_north</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="10000000.000000">
        <cd:Unit>degrees_north</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="err_tangent_long">
    <cd:Integer>
      <cd:Description>Error on the longitude of the tangent point value provided during the measurement, generally at half-measurement (the temporal shift is given by the time shift for the ray tracing in the SPH)</cd:Description>
      <cd:Unit>1e-7 degrees_east</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="10000000.000000">
        <cd:Unit>degrees_east</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="err_tangent_alt">
    <cd:Integer>
      <cd:Description>Error on the altitude of the tangent point value provided during the measurement, generally at half-measurement (the temporal shift is given by the time shift for the ray tracing in the SPH)</cd:Description>
      <cd:Unit>1e-3 m</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="1000.000000">
        <cd:Unit>m</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>uint32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="tangent_atm_p">
    <cd:Float>
      <cd:Description>Tangent point atmospheric pressure (from external model)</cd:Description>
      <cd:Unit>Pa</cd:Unit>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>float</cd:NativeType>
    </cd:Float>
  </cd:Field>
  <cd:Field name="tangent_atm_temp">
    <cd:Float>
      <cd:Description>Tangent point temperature (from external model)</cd:Description>
      <cd:Unit>K</cd:Unit>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>float</cd:NativeType>
    </cd:Float>
  </cd:Field>
  <cd:Field name="tangent_density">
    <cd:Float>
      <cd:Description>Tangent point density (from external model)</cd:Description>
      <cd:Unit>1/cm3</cd:Unit>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>float</cd:NativeType>
    </cd:Float>
  </cd:Field>
  <cd:Field name="spec_grid">
    <cd:Array>
      <cd:Description>Spectral grid correction for the transmission model function computed from retrieved vertical profiles</cd:Description>
      <cd:Dimension>2336</cd:Dimension>
      <cd:Integer>
        <cd:Unit>1e-3 nm</cd:Unit>
        <cd:Conversion numerator="1.000000" denominator="1000.000000">
          <cd:Unit>nm</cd:Unit>
        </cd:Conversion>
        <cd:BitSize>16</cd:BitSize>
        <cd:NativeType>uint16</cd:NativeType>
      </cd:Integer>
    </cd:Array>
  </cd:Field>
</cd:Record>

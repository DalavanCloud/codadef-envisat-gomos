<?xml version="1.0"?>
<cd:Record format="binary" name="GOM_NL__2P_ADSR_geolocation_v0" last-modified="2012-10-03" xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07">
  <cd:Field name="dsr_time">
    <cd:Time timeformat="binary_envisat_datetime">
      <cd:Description>Start time of the measurement</cd:Description>
    </cd:Time>
  </cd:Field>
  <cd:Field name="attach_flag">
    <cd:Integer>
      <cd:Description>Attachment flag. 1 = all MDSRs corresponding to this ADSR are blank; 0 = otherwise.</cd:Description>
      <cd:BitSize>8</cd:BitSize>
      <cd:NativeType>uint8</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="lat">
    <cd:Integer>
      <cd:Description>Latitude of the spacecraft. Value provided during the measurement, generally at half-measurement (the temporal shift is given by the time shift for the ray tracing.</cd:Description>
      <cd:Unit>1e-6 degrees_north</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="1000000.000000">
        <cd:Unit>degrees_north</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="longit">
    <cd:Integer>
      <cd:Description>Longitude of the spacecraft. Value provided during the measurement, generally at half-measurement (the temporal shift is given by the time shift for the ray tracing.</cd:Description>
      <cd:Unit>1e-6 degrees_east</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="1000000.000000">
        <cd:Unit>degrees_east</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="alt">
    <cd:Integer>
      <cd:Description>Altitude of the spacecraft. Value provided during the measurement, generally at half-measurement (the temporal shift is given by the time shift for the ray tracing.</cd:Description>
      <cd:Unit>1e-2 m</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="100.000000">
        <cd:Unit>m</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>uint32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="tangent_lat">
    <cd:Integer>
      <cd:Description>Latitude of the tangent point. Value provided during the measurement, generally at half-measurement (the temporal shift is given by the time shift for the ray tracing.</cd:Description>
      <cd:Unit>1e-6 degrees_north</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="1000000.000000">
        <cd:Unit>degrees_north</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="tangent_long">
    <cd:Integer>
      <cd:Description>Longitude of the tangent point. Value provided during the measurement, generally at half-measurement (the temporal shift is given by the time shift for the ray tracing.</cd:Description>
      <cd:Unit>1e-6 degrees_east</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="1000000.000000">
        <cd:Unit>degrees_east</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="tangent_alt">
    <cd:Integer>
      <cd:Description>Altitude of the tangent point. Value provided during the measurement, generally at half-measurement (the temporal shift is given by the time shift for the ray tracing.</cd:Description>
      <cd:Unit>1e-2 m</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="100.000000">
        <cd:Unit>m</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>uint32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="err_tangent_lat">
    <cd:Integer>
      <cd:Description>Error on the latitude of the tangent point. Value provided during the measurement, generally at half-measurement (the temporal shift is given by the time shift for the ray tracing.</cd:Description>
      <cd:Unit>1e-7 degrees_north</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="10000000.000000">
        <cd:Unit>degrees_north</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="err_tangent_long">
    <cd:Integer>
      <cd:Description>Error on the longitude of the tangent point. Value provided during the measurement, generally at half-measurement (the temporal shift is given by the time shift for the ray tracing.</cd:Description>
      <cd:Unit>1e-7 degrees_east</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="10000000.000000">
        <cd:Unit>degrees_east</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="err_tangent_alt">
    <cd:Integer>
      <cd:Description>Error on the altitude of the tangent point. Value provided during the measurement, generally at half-measurement (the temporal shift is given by the time shift for the ray tracing.</cd:Description>
      <cd:Unit>1e-3 m</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="1000.000000">
        <cd:Unit>m</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>uint32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="tangent_atm_p">
    <cd:Float>
      <cd:Description>Tangent point atmospheric pressure (from external model)</cd:Description>
      <cd:Unit>Pa</cd:Unit>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>float</cd:NativeType>
    </cd:Float>
  </cd:Field>
  <cd:Field name="tangent_temp">
    <cd:Float>
      <cd:Description>Tangent point temperature (from external model)</cd:Description>
      <cd:Unit>K</cd:Unit>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>float</cd:NativeType>
    </cd:Float>
  </cd:Field>
  <cd:Field name="air_density">
    <cd:Float>
      <cd:Description>Local air density from GOMOS atmospheric profile</cd:Description>
      <cd:Unit>1/cm3</cd:Unit>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>float</cd:NativeType>
    </cd:Float>
  </cd:Field>
  <cd:Field name="air_density_std">
    <cd:Integer>
      <cd:Description>Standard deviation for the local air density. A value of 65535 indicates the value is invalid.</cd:Description>
      <cd:Unit>1e-1 %</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="10.000000">
        <cd:Unit>%</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>16</cd:BitSize>
      <cd:NativeType>uint16</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="local_temp">
    <cd:Float>
      <cd:Description>Local temperature</cd:Description>
      <cd:Unit>K</cd:Unit>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>float</cd:NativeType>
    </cd:Float>
  </cd:Field>
  <cd:Field name="local_temp_std">
    <cd:Integer>
      <cd:Description>Standard deviation for the local temperature. A value of 65535 indicates the value is invalid.</cd:Description>
      <cd:Unit>1e-1 %</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="10.000000">
        <cd:Unit>%</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>16</cd:BitSize>
      <cd:NativeType>uint16</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="pcd">
    <cd:Integer>
      <cd:Description>PCD summary. 0 = atmospheric processing was valid; other value = atmospheric processing was not valid.</cd:Description>
      <cd:BitSize>8</cd:BitSize>
      <cd:NativeType>uint8</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="spare_1">
    <cd:Raw>
      <cd:Description>Spare</cd:Description>
      <cd:BitSize>64</cd:BitSize>
    </cd:Raw>
    <cd:Hidden/>
  </cd:Field>
</cd:Record>

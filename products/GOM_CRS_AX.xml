  <cd:ProductVariable name="ds_to_dsd_index">
    <cd:Dimension><![CDATA[$num_ds]]></cd:Dimension>
    <cd:Init><![CDATA[$ds_to_dsd_index[0] = index(/dsd, str(./ds_name) == "CRS_O3_CROSS_SECT_SPA_GADS  "); $ds_to_dsd_index[1] = index(/dsd, str(./ds_name) == "CRS_O3_CROSS_SECT_SPB_GADS  "); $ds_to_dsd_index[2] = index(/dsd, str(./ds_name) == "CRS_NO2_CROSS_SECT_GADS     "); $ds_to_dsd_index[3] = index(/dsd, str(./ds_name) == "CRS_NO3_CROSS_SECT_GADS     "); $ds_to_dsd_index[4] = index(/dsd, str(./ds_name) == "CRS_OCLO_CROSS_SECT_GADS    "); $ds_to_dsd_index[5] = index(/dsd, str(./ds_name) == "CRS_O2_CROSS_SECT_GADS      "); $ds_to_dsd_index[6] = index(/dsd, str(./ds_name) == "CRS_H2O_CROSS_SECT_GADS     "); $ds_to_dsd_index[7] = index(/dsd, str(./ds_name) == "CRS_O3_CROSS_SECT_SPA_MDS   "); $ds_to_dsd_index[8] = index(/dsd, str(./ds_name) == "CRS_O3_CROSS_SECT_SPB_MDS   "); $ds_to_dsd_index[9] = index(/dsd, str(./ds_name) == "CRS_NO2_CROSS_SECT_MDS      "); $ds_to_dsd_index[10] = index(/dsd, str(./ds_name) == "CRS_NO3_CROSS_SECT_MDS      "); $ds_to_dsd_index[11] = index(/dsd, str(./ds_name) == "CRS_OCLO_CROSS_SECT_MDS     "); $ds_to_dsd_index[12] = index(/dsd, str(./ds_name) == "CRS_O2_CROSS_SECT_MDS       "); $ds_to_dsd_index[13] = index(/dsd, str(./ds_name) == "CRS_H2O_CROSS_SECT_MDS      ")]]></cd:Init>
  </cd:ProductVariable>
  <cd:ProductVariable name="ds_available">
    <cd:Dimension><![CDATA[$num_ds]]></cd:Dimension>
    <cd:Init><![CDATA[for i = 0 to $num_ds - 1 do $ds_available[i] = if($ds_to_dsd_index[i] != -1 && !(bytes(/dsd[$ds_to_dsd_index[i]]/filename, 8) == "NOT USED"), 1, 0)]]></cd:Init>
  </cd:ProductVariable>
  <cd:ProductVariable name="ds_offset">
    <cd:Dimension><![CDATA[$num_ds]]></cd:Dimension>
    <cd:Init><![CDATA[for i = 0 to $num_ds - 1 do $ds_offset[i] = 8 * int(/dsd[$ds_to_dsd_index[i]]/ds_offset)]]></cd:Init>
  </cd:ProductVariable>
  <cd:ProductVariable name="num_dsr">
